{
	"name": "Ethermine Watcher",
	"description": "Watch your Ethermine hashrate, balance, ETA until payout and more.",
	"manifest_version": 2,
	"version": "1.0.8",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"storage",
		"alarms",
		"notifications"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["bignumber.min.js","global.js","event.js"],
		"persistent": false
	}
}
